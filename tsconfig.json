{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "allowJs": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist"
    // "noEmit": true
  },
  "include": ["src/**/*"]
}
